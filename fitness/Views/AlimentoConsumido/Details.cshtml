@model Fitness.Model.Models.AlimentoConsumido



<div>
    <h4>Alimento de la dieta</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Alimento
        </dt>
        <dd class="col-sm-10">
            @(Model.AlimentoNavigation.Descripcion)
        </dd>
        <dt class="col-sm-2">
           Plato
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Opcion)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id">Editar</a> |
    <a asp-action="Index">Regresar</a>
</div>

